{
  "name": "appweb",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\"",
    "vercel-build": "npm run build:web"
  },
  "devDependencies": {
    "turbo": "^2.5.5",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  }
}
